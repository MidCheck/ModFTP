#include<iostream>
using namespace std;

int main(){
	const char* str = "Helo";
	switch(str){
		case "Helo":
			cout << "str" <<endl;
			break;
		case "Word":
			cout << "world" <<endl;
			break;
		default:
			cout << "default" <<endl;
	}
	return 0;
}
